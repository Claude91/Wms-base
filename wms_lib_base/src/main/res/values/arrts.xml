<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NameTextView">
        <attr name="name" format="string" />
    </declare-styleable>

    <declare-styleable name="TitleView">
        <attr name="title" format="string" />
        <attr name="rightText" format="string" />
        <attr name="backIcon" format="reference" />
        <attr name="hideBackIcon" format="boolean" />
        <attr name="rightIcon" format="reference" />
        <attr name="showRightIcon" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LabelTextView">
        <attr name="labelStyle">
            <enum name="bold" value="100"/>
        </attr>
        <attr name="labelText" format="string" />
        <attr name="symbolText" format="string" />
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="size" format="dimension" />
        <attr name="labelSize" format="dimension" />
        <attr name="textMarginLeft" format="dimension" />
        <attr name="symbolTextMarginLeft" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="color" format="color" />
        <attr name="labelColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ImageTextButton">
        <attr name="src" format="reference" />
        <attr name="labelT" format="string" />
        <attr name="labelTColor" format="reference" />
        <attr name="labelTSize" format="dimension" />
        <attr name="isSelect" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SystemEditText">
        <attr name="hintContent" format="string" />
    </declare-styleable>

</resources>